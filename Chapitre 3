Exercice 11

from robot import *

valeur = 0

for loop in range(10):
   valeur = valeur + 1
   for loop in range(valeur):
      droite()
   ramasser()
   for loop in range(valeur): 
      gauche()
   deposer()
   
Exercice 12

Taille = 1
Cube = 0
for loop in range(9):
   Cube = Cube + Taille ** 3
   Taille = Taille + 2
print(Cube)

Exercice 13

Ligne = 1
for loop in range(20):
   Colonnes = 1
   for loop in range(20):
      print(Colonnes * Ligne, end = " ")
      Colonnes = Colonnes + 1
   print()
   Ligne = Ligne + 1
